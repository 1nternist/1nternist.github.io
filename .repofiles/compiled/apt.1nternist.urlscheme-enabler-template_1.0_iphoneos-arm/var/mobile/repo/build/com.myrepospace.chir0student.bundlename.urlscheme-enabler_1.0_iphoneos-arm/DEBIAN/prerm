#!/bin/sh

killall $CFBundleName

export $CFBundleName_INFO_PLIST="/Applications/AppName.app/Info.plist"
export $CFBundleName_BAK_PLIST="/Applications/AppName.app/Info.plist.bak"

if [ -e $CFBundleName_BAK_PLIST ]
then
    cp $CFBundleName_BAK_PLIST $CFBundleName_INFO_PLIST
    rm $CFBundleName_BAK_PLIST

    echo "Restored Info.plist file of $CFBundleName"
fi